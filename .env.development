# .env.development - Development Environment
# ============================================

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_TIMEOUT=30000
VITE_UPLOAD_TIMEOUT=600000

# Authentication
VITE_JWT_STORAGE_KEY=screen_capture_auth_token_dev
VITE_JWT_EXPIRES_CHECK=true

# File Upload Settings
VITE_MAX_FILE_SIZE=52428800
VITE_ALLOWED_IMAGE_TYPES=image/png,image/jpeg,image/webp,image/gif
VITE_ALLOWED_VIDEO_TYPES=video/webm,video/mp4,video/quicktime
VITE_UPLOAD_CHUNK_SIZE=5242880

# Development Settings
VITE_NODE_ENV=development
VITE_DEBUG_MODE=true
VITE_ENABLE_CONSOLE_LOGS=true
VITE_MOCK_API_DELAY=500

# Storage & Cache
VITE_ENABLE_LOCAL_CACHE=true
VITE_CACHE_DURATION=1800000
